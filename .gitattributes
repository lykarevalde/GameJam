# Auto detect text files and perform LF normalization
* text=auto
*.ogg filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
assets/spooksoundss/*.ogg filter=lfs diff=lfs merge=lfs -text
assets/spooksoundss/*.mp3 filter=lfs diff=lfs merge=lfs -text
assets/spooksoundss/*.wav filter=lfs diff=lfs merge=lfs -text
